-- Creacion de la base de dtos
CREATE DATABASE pruebadb;
USE pruebadb;


--Creacion de tabla
CREATE TABLE pruebadb (
    id INT AUTO_INCREMENT PRIMARY KEY,
    login VARCHAR(255) NOT NULL,
    cluster VARCHAR(50) NOT NULL,
    Njobs INT NOT NULL,
    Nhoras INT NOT NULL,
    mes INT NOT NULL,
    anio INT NOT NULL
);

